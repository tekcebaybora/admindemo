<!DOCTYPE html>

@model AdminPseudoscops.Models.AppModel.Devices
@{
    ViewBag.Title = "Manage";
}
<!--[if !IE]><!-->
<html lang="en">

<meta http-equiv="content-type" content="text/html;charset=UTF-8" /><!-- /Added by HTTrack -->
<head>

</head>
<!-- END HEAD -->
<body class="page-header-fixed page-sidebar-closed-hide-logo  page-container-bg-solid page-content-white">

    <div class="page-content-wrapper">
        <!-- BEGIN CONTENT BODY -->
        <div class="page-content">
            <!-- BEGIN PAGE HEADER-->
            <!-- BEGIN PAGE TITLE-->
            <h3 class="page-title">
                Yönetici Hesabı

            </h3>
            <!-- END PAGE TITLE-->
            <!-- END PAGE HEADER-->
            <div class="row">
                <div class="col-md-12">
                    <!-- BEGIN PROFILE SIDEBAR -->
                    <!-- END BEGIN PROFILE SIDEBAR -->
                    <!-- BEGIN PROFILE CONTENT -->
                    <div class="profile-content">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="portlet light ">
                                    <div class="portlet-title tabbable-line">
                                        <div class="caption caption-md">
                                            <i class="icon-globe theme-font hide"></i>
                                            <span class="caption-subject font-blue-madison bold uppercase">Hesap Profili</span>
                                        </div>
                                        <ul class="nav nav-tabs">
                                            <li class="active">
                                                <a href="#tab_1_1" data-toggle="tab">Cihaz Ekle</a>
                                            </li>
                                            @*<li>
                                                <a href="#tab_1_2" data-toggle="tab">Lokasyon Ekle</a>
                                            </li>*@
                                        </ul>
                                    </div>

                                    <div class="portlet-body">
                                        <div class="tab-content">
                                            <!-- PERSONAL INFO TAB -->
                                           @using (Html.BeginForm("Create", "Devices", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                                           {
                                            @Html.AntiForgeryToken()
                                                @Html.HiddenFor(model => model.DeviceKey)
                                                <div class="tab-pane active" id="tab_1_1">
                                                    <form role="form" action="#">
                                                        <div class="form-group">
                                                            <label class="control-label">Cihaz İsmi</label>
                                                            <input type="text" placeholder="Cihaz İsmi" id="DeviceId" name="DeviceId" class="form-control" />
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label">Yeni Lokasyon</label>
                                                            <input type="text" placeholder="Soyisim" id="Location"  name="Location"class="form-control" />
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label ">Statü</label>
                                                            <select class="bs-select form-control" name="IsActive" id="IsActive">
                                                                <option value=0>Pasif</option>
                                                                <option value=1>Aktif</option>
                                                               
                                                        
                                                            </select>
                                                        </div>
                                                       
                                                        <div class="pull-left mt15 mb15 col-md-2 ">
                                                                                                           
                                                           
                                                            <a href="/Devices/Devices" class="btn default"> Cancel (İptal Et) </a>
                                                        </div>
                                                        <div class="pull-right mt15 mb15 col-md-2 ">

                                                            <button class="btn green" type="submit">Save Changes (Kaydet)</button>

                                                        </div>
                                                        <br />
                                                    </form>
                                                </div>
                                            }
                                            <!-- END PERSONAL INFO TAB -->
                                            <!-- CHANGE AVATAR TAB -->
                                            <div class="tab-pane" id="tab_1_2">
                                                <p>
                                                    Lütfen bir profil fotografı ekleyin
                                                </p>
                                                <form action="#" role="form">
                                                    <div class="form-group">
                                                        <div class="fileinput fileinput-new" data-provides="fileinput">
                                                            <div class="fileinput-new thumbnail" style="width: 200px; height: 150px;">
                                                                <img src="http://www.placehold.it/200x150/EFEFEF/AAAAAA&amp;text=no+image" alt="" />
                                                            </div>
                                                            <div class="fileinput-preview fileinput-exists thumbnail" style="max-width: 200px; max-height: 150px;"> </div>
                                                            <div>
                                                                <span class="btn default btn-file">
                                                                    <span class="fileinput-new"> Select image </span>
                                                                    <span class="fileinput-exists"> Change </span>
                                                                    <input type="file" name="...">
                                                                </span>
                                                                <a href="javascript:;" class="btn default fileinput-exists" data-dismiss="fileinput"> Remove </a>
                                                            </div>
                                                        </div>
                                                        <div class="clearfix margin-top-10">
                                                            <span class="label label-danger">NOTE! </span>
                                                            <span>  Attached image thumbnail is supported in Latest Firefox, Chrome, Opera, Safari and Internet Explorer 10 only </span>
                                                        </div>
                                                    </div>
                                                    <div class="margin-top-10">
                                                        <a href="javascript:;" class="btn green"> Submit (Gönder) </a>
                                                        <a href="javascript:;" class="btn default"> Cancel (İptal Et) </a>
                                                    </div>
                                                </form>
                                            </div>
                                            <!-- END CHANGE AVATAR TAB -->
                                            <!-- CHANGE PASSWORD TAB -->
                                            <div class="tab-pane" id="tab_1_3">
                                                <form action="#">
                                                    <div class="form-group">
                                                        <label class="control-label">Current Password</label>
                                                        <input type="password" placeholder="Güncel Parolanız" class="form-control" />
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="control-label">New Password</label>
                                                        <input type="password" placeholder="Yeni Parolanız" class="form-control" />
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="control-label">Re-type New Password</label>
                                                        <input type="password" placeholder="Tekrar Yeni Parolanız" class="form-control" />
                                                    </div>
                                                    <div class="margin-top-10">
                                                        <a href="javascript:;" class="btn green"> Change Password (Parolayı Değiştir) </a>
                                                        <a href="javascript:;" class="btn default"> Cancel (İptal Et) </a>
                                                    </div>
                                                </form>
                                            </div>
                                            <!-- END CHANGE PASSWORD TAB -->
                                            <!-- PRIVACY SETTINGS TAB -->
                                            <div class="tab-pane" id="tab_1_4">
                                                <form action="#">
                                                    <table class="table table-light table-hover">
                                                        <tr>
                                                            <td> Anim pariatur cliche reprehenderit, enim eiusmod high life accusamus.. </td>
                                                            <td>
                                                                <label class="uniform-inline">
                                                                    <input type="radio" name="optionsRadios1" value="option1" /> Yes
                                                                </label>
                                                                <label class="uniform-inline">
                                                                    <input type="radio" name="optionsRadios1" value="option2" checked /> No
                                                                </label>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td> Enim eiusmod high life accusamus terry richardson ad squid wolf moon </td>
                                                            <td>
                                                                <label class="uniform-inline">
                                                                    <input type="checkbox" value="" /> Yes
                                                                </label>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td> Enim eiusmod high life accusamus terry richardson ad squid wolf moon </td>
                                                            <td>
                                                                <label class="uniform-inline">
                                                                    <input type="checkbox" value="" /> Yes
                                                                </label>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td> Enim eiusmod high life accusamus terry richardson ad squid wolf moon </td>
                                                            <td>
                                                                <label class="uniform-inline">
                                                                    <input type="checkbox" value="" /> Yes
                                                                </label>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                    <!--end profile-settings-->
                                                    <div class="margin-top-10">
                                                        <a href="javascript:;" class="btn red"> Save Changes </a>
                                                        <a href="javascript:;" class="btn default"> Cancel </a>
                                                    </div>
                                                </form>
                                            </div>
                                            <!-- END PRIVACY SETTINGS TAB -->
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END PROFILE CONTENT -->
                </div>
            </div>
        </div>
        <!-- END CONTENT BODY -->
    </div>
    <!-- END CON
        TENT -->
    <!-- END CONTAINER -->
